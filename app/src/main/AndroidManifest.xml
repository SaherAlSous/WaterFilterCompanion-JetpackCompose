<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.saher.android.waterfiltercompanion_jetpackcompose">

    <application
        android:name=".Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_lbaseline_local_dring_24"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_lbaseline_local_dring_24_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WaterFilterCompanionJetpackCompose">
        <activity
            android:name=".ui.screen.main.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.WaterFilterCompanionJetpackCompose.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>